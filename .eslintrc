{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended",
                "plugin:cypress/recommended",
                "plugin:prettier/recommended"],

    "plugins": ["prettier", "cypress"],

    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "prettier/prettier": "error"
    }
}
